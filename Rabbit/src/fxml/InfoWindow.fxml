<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox prefHeight="320.0" prefWidth="289.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="40.0" left="10.0" right="10.0" top="40.0" />
         </BorderPane.margin>
         <children>
            <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
            </ImageView>
            <TextField fx:id="titleText" alignment="CENTER" editable="false" focusTraversable="false" prefHeight="57.0" prefWidth="296.0" style="-fx-background-color: transparent;" text="Crazy Rabbit">
               <font>
                  <Font name="Harrington" size="30.0" />
               </font>
            </TextField>
            <TextArea fx:id="describeText" editable="false" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: transparent;" wrapText="true">
               <font>
                  <Font name="Georgia" size="15.0" />
               </font>
            </TextArea>
         </children>
      </VBox>
   </center>
   <left>
      <Button fx:id="leftButton" maxHeight="40.0" maxWidth="60.0" minHeight="40.0" minWidth="60.0" mnemonicParsing="false" onAction="#leftButtonPressed" prefHeight="40.0" prefWidth="60.0" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Button fx:id="rightButton" maxHeight="40.0" maxWidth="60.0" minHeight="40.0" minWidth="60.0" mnemonicParsing="false" onAction="#rightButtonPressed" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
